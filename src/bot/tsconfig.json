{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ES2022",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": true
  },
  "include": ["src", "typedefs", "langchain"],
  "ts-node": {
    "esm": true
  }
}
