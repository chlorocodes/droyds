{
  "name": "@lyme/server",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run compile && npm run copyFiles",
    "compile": "tsc",
    "copyFiles": "cp -r src/assets dist/assets",
    "clean": "rm -r dist",
    "dev": "nodemon --files src/index.ts",
    "start": "node dist/index.ts",
    "migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/session": "^10.4.0",
    "@fastify/static": "^6.10.2",
    "@fastify/type-provider-typebox": "^3.2.0",
    "@lyme/bot": "1.0.0",
    "@prisma/client": "^4.16.1",
    "@sinclair/typebox": "^0.28.19",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "prisma": "^4.16.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
