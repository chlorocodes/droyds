generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}

model Session {
  id          String   @id @default(uuid()) @map("id")
  userId      String   @map("user_id")
  accessToken String   @map("access_token")
  scope       String   @map("scope")
  expires     DateTime @map("expires")

  @@map("sessions")
}
